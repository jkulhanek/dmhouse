models/players/crash/redskin // crash red shader
{
    cull disable
    nopicmip
    {
        map models/players/crash/redskin.tga
        blendfunc blend
        alphaFunc GE128
        depthwrite
    }
    {
        map models/players/crash/skin1_scroll.tga
        blendfunc add
        tcMod scroll -1.6 0
    }
    {
        map models/players/crash/thruster_glow.tga
        blendfunc add
    }
}

